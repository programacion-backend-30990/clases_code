<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head') %>
</head>


<body>
    <header>
        <%- include('./partials/header') %>
    </header>
    
    <h2><%= mensaje %></h2>

    <ul>
        <% pets.forEach( function(pet) { %>
            <li>
                <strong><%= pet.name %></strong>
                <i><%= pet.animal %></i>
                y tiene <%= pet.months %> meses.
            </li>
        <% }) %>
    </ul>

    <footer>
        <%- include('./partials/footer') %>
    </footer>

</body>
</html>